{
  "name": "gymtracker",
  "version": "1.0.0",
  "description": "GymTracker - Dual Interface Fitness Management System with Node.js + SQLite + Apache",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:init": "node -e \"require('./config/database').initDatabase().then(() => console.log('Database initialized')).catch(console.error)\"",
    "db:backup": "node -e \"const fs = require('fs'); const src = process.env.DATABASE_PATH || './database/gymtracker.db'; const dest = `./backups/gymtracker-${new Date().toISOString().split('T')[0]}.db`; fs.mkdirSync('./backups', {recursive: true}); fs.copyFileSync(src, dest); console.log('Database backed up to:', dest);\"",
    "lint": "echo \"Linting not configured yet\"",
    "format": "echo \"Formatting not configured yet\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szanella68/gymtracker.git"
  },
  "keywords": [
    "fitness",
    "gym",
    "tracker",
    "workout",
    "trainer",
    "client",
    "management",
    "health",
